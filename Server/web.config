<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="Main.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="NodeRoute" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="Main.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
